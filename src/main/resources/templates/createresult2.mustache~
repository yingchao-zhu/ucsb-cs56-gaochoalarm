{{> head }}
<body>
 {{> nav }}
 <p>
     This page has the result of creating a new alarm.
 </p>
<input type="text" value="Key" id="myInput">
<button onclick="myFunction()">Copy</button>

<script>
function myFunction() {
  var copyText = document.getElementById("myInput");
  copyText.select();
  document.execCommand("copy");
  alert("Copied the text: " + copyText.value);
}
</script>
